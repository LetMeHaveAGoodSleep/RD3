                                                        Height="30"
                                                        hc:BorderElement.CornerRadius="4"
                                                        hc:VisualElement.HighlightBackground="{DynamicResource PrimaryBrush}"
                                                        hc:VisualElement.HighlightBorderBrush="{DynamicResource PrimaryBrush}"
                                                        hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                        Background="{DynamicResource DefaultBrush}"
                                                        Content="{ex:Language Key='Associated'}"
                                                        FontSize="{DynamicResource TextFontSize}"
                                                        Foreground="{DynamicResource SecondaryTextBrush}"
                                                        IsChecked="{Binding CurrentDeviceParameter.O2Param.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Associated}" />
                                                    <RadioButton
                                                        Width="60"
                                                        Height="30"
                                                        hc:BorderElement.CornerRadius="4"
                                                        hc:VisualElement.HighlightBackground="#3C739A"
                                                        hc:VisualElement.HighlightBorderBrush="#3C739A"
                                                        hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                        Background="{DynamicResource DefaultBrush}"
                                                        Content="{ex:Language Key='Free'}"
                                                        FontSize="{DynamicResource TextFontSize}"
                                                        Foreground="{DynamicResource SecondaryTextBrush}"
                                                        IsChecked="{Binding CurrentDeviceParameter.O2Param.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Free}"
                                                        Visibility="Collapsed" />
                                                    <RadioButton
                                                        Width="80"
                                                        Height="30"
                                                        Margin="5,0,0,0"
                                                        hc:BorderElement.CornerRadius="4"
                                                        hc:VisualElement.HighlightBackground="{DynamicResource ThirdlyTextBrush}"
                                                        hc:VisualElement.HighlightBorderBrush="{DynamicResource ThirdlyTextBrush}"
                                                        hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                        Background="{DynamicResource DefaultBrush}"
                                                        Content="{ex:Language Key='Disable'}"
                                                        FontSize="{DynamicResource TextFontSize}"
                                                        Foreground="{DynamicResource SecondaryTextBrush}"
                                                        IsChecked="{Binding CurrentDeviceParameter.O2Param.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Disable}" />
                                                </hc:ButtonGroup>
                                            </DockPanel>
                                        </hc:UniformSpacingPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{ex:Language Key='Alarm'}" />
                                            <Line
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Stroke="Gray"
                                                StrokeThickness="1"
                                                X1="10"
                                                X2="220"
                                                Y1="0"
                                                Y2="0" />
                                        </StackPanel>
                                        <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="5">
                                            <DockPanel LastChildFill="False" Visibility="Collapsed">
                                                <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='dBand:'}" />
                                                <TextBox
                                                    Width="70"
                                                    VerticalAlignment="Center"
                                                    hc:InfoElement.Placeholder="{ex:Language Key='Set value'}"
                                                    Style="{StaticResource TextBoxExtend}"
                                                    Text="" />
                                            </DockPanel>
                                            <DockPanel LastChildFill="False" Visibility="Collapsed">
                                                <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='LL:'}" />
                                                <TextBox
                                                    Width="70"
                                                    VerticalAlignment="Center"
                                                    hc:InfoElement.Placeholder="{ex:Language Key='Set value'}"
                                                    Style="{StaticResource TextBoxExtend}"
                                                    Text="" />
                                            </DockPanel>
                                            <DockPanel LastChildFill="False">
                                                <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='Set value(SV):'}" />
                                                <TextBox
                                                    Width="90"
                                                    Height="30"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    hc:InfoElement.Placeholder="{ex:Language Key='unit:L/min'}"
                                                    Style="{StaticResource TextBoxExtend}"
                                                    Text="{Binding CurrentDeviceParameter.O2Param.FlowSpeed}" />
                                            </DockPanel>
                                        </hc:UniformSpacingPanel>
                                    </hc:UniformSpacingPanel>
                                    <hc:UniformSpacingPanel
                                        Orientation="Vertical"
                                        Spacing="10"
                                        Visibility="Collapsed">
                                        <TextBlock Text="{ex:Language Key='Associative modules:'}" />
                                        <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="10">
                                            <hc:UniformSpacingPanel Orientation="Vertical" Spacing="5">
                                                <Border
                                                    Width="110"
                                                    Height="120"
                                                    Padding="5"
                                                    BorderBrush="{DynamicResource BorderBrush}"
                                                    BorderThickness="0.5"
                                                    CornerRadius="4">
                                                    <hc:UniformSpacingPanel Orientation="Vertical" Spacing="5">
                                                        <CheckBox Content="{ex:Language Key='DO'}" IsChecked="True" />
                                                    </hc:UniformSpacingPanel>
                                                </Border>
                                            </hc:UniformSpacingPanel>
                                        </hc:UniformSpacingPanel>
                                    </hc:UniformSpacingPanel>
                                </hc:UniformSpacingPanel>
                            </TabItem>
                        </TabControl>
                    </Border>
                </hc:UniformSpacingPanel>
                <DockPanel
                    VerticalAlignment="Bottom"
                    DockPanel.Dock="Right"
                    LastChildFill="False">
                    <hc:UniformSpacingPanel
                        DockPanel.Dock="Right"
                        Orientation="Horizontal"
                        Spacing="10">
                        <Button
                            Width="80"
                            Height="30"
                            HorizontalAlignment="Right"
                            HorizontalContentAlignment="Center"
                            hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                            BorderBrush="{StaticResource PrimaryBrush}"
                            Command="{Binding CancelCommand}"
                            Content="{ex:Language Key='Cancel'}"
                            FontSize="{DynamicResource NormalFontSize}"
                            Foreground="{StaticResource PrimaryBrush}" />
                        <Button
                            Width="80"
                            Height="30"
                            Margin="0,0,5,0"
                            HorizontalAlignment="Right"
                            HorizontalContentAlignment="Center"
                            hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                            Background="{StaticResource PrimaryBrush}"
                            BorderBrush="{StaticResource PrimaryBrush}"
                            Command="{Binding ApplyCommand}"
                            Content="{ex:Language Key='Apply'}"
                            FontSize="{DynamicResource NormalFontSize}"
                            Foreground="White"
                            Visibility="{Binding Enable, Converter={StaticResource BooleanToVisibilityConverter}}" />
                    </hc:UniformSpacingPanel>
                </DockPanel>
            </hc:UniformSpacingPanel>
        </Grid>
    </Border>
</UserControl>
