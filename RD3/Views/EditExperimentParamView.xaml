                        Height="320"
                        Style="{StaticResource BorderCard}">
                        <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="20">
                            <hc:UniformSpacingPanel
                                Margin="5"
                                Orientation="Vertical"
                                Spacing="15">
                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="20">
                                    <TextBlock FontSize="{DynamicResource HeadFontSize}" Text="{ex:Language Key='PH'}" />
                                    <Button
                                        hc:BorderElement.Circular="True"
                                        hc:BorderElement.CornerRadius="16"
                                        Command="{Binding PHMVCommand}"
                                        Content="{ex:Language Key='策略编辑'}"
                                        FontSize="{DynamicResource NormalFontSize}"
                                        Foreground="{StaticResource PrimaryBrush}"
                                        Visibility="{Binding CurrentDeviceParameter.PHParam.ControlMode, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Associated}" />
                                </hc:UniformSpacingPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="{ex:Language Key='Control'}" />
                                    <Line
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Stroke="Gray"
                                        StrokeThickness="1"
                                        X1="10"
                                        X2="240"
                                        Y1="0"
                                        Y2="0" />
                                </StackPanel>
                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="30">
                                    <DockPanel LastChildFill="False">
                                        <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='Status:'}" />
                                        <hc:ButtonGroup Style="{StaticResource ButtonGroupSolid}">
                                            <RadioButton
                                                Width="80"
                                                Height="30"
                                                Margin="-5,0,0,0"
                                                hc:BorderElement.CornerRadius="4"
                                                hc:VisualElement.HighlightBackground="{DynamicResource SuccessBrush}"
                                                hc:VisualElement.HighlightBorderBrush="{DynamicResource SuccessBrush}"
                                                hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                Background="{DynamicResource DefaultBrush}"
                                                Content="{ex:Language Key='时间序列'}"
                                                FontSize="{DynamicResource TextFontSize}"
                                                Foreground="{DynamicResource SecondaryTextBrush}"
                                                IsChecked="{Binding CurrentDeviceParameter.PHParam.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Enable}" />
                                            <RadioButton
                                                Width="80"
                                                Height="30"
                                                hc:BorderElement.CornerRadius="4"
                                                hc:VisualElement.HighlightBackground="{DynamicResource PrimaryBrush}"
                                                hc:VisualElement.HighlightBorderBrush="{DynamicResource PrimaryBrush}"
                                                hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                Background="{DynamicResource DefaultBrush}"
                                                Content="{ex:Language Key='策略'}"
                                                FontSize="{DynamicResource TextFontSize}"
                                                Foreground="{DynamicResource SecondaryTextBrush}"
                                                IsChecked="{Binding CurrentDeviceParameter.PHParam.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Associated}" />
                                            <RadioButton
                                                Width="80"
                                                Height="30"
                                                Margin="5,0,0,0"
                                                hc:BorderElement.CornerRadius="4"
                                                hc:VisualElement.HighlightBackground="{DynamicResource ThirdlyTextBrush}"
                                                hc:VisualElement.HighlightBorderBrush="{DynamicResource ThirdlyTextBrush}"
                                                hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                Background="{DynamicResource DefaultBrush}"
                                                Content="{ex:Language Key='Disable'}"
                                                FontSize="{DynamicResource TextFontSize}"
                                                Foreground="{DynamicResource SecondaryTextBrush}"
                                                IsChecked="{Binding CurrentDeviceParameter.PHParam.ControlMode, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Disable}" />
                                        </hc:ButtonGroup>
                                    </DockPanel>
                                    <hc:UniformSpacingPanel
                                        VerticalAlignment="Bottom"
                                        Orientation="Vertical"
                                        Spacing="5"
                                        Visibility="Collapsed">
                                        <CheckBox
                                            VerticalAlignment="Center"
                                            Content="{ex:Language Key='Acid'}"
                                            IsChecked="{Binding CurrentDeviceParameter.PHParam.AcidAssociated}" />
                                        <CheckBox Content="{ex:Language Key='Base'}" IsChecked="{Binding CurrentDeviceParameter.PHParam.BaseAssociated}" />

                                    </hc:UniformSpacingPanel>
                                </hc:UniformSpacingPanel>
                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="10">
                                    <DockPanel LastChildFill="False">
                                        <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='设定值：'}" />
                                        <TextBox
                                            Width="80"
                                            Height="30"
                                            VerticalAlignment="Center"
                                            HorizontalContentAlignment="Center"
                                            hc:InfoElement.Placeholder="{ex:Language Key='Set value'}"
                                            Style="{StaticResource TextBoxExtend}"
                                            Text="{Binding CurrentDeviceParameter.PHParam.PH_PV}" />
                                    </DockPanel>
                                    <DockPanel LastChildFill="False">
                                        <TextBlock DockPanel.Dock="Top" Text="{ex:Language Key='dBand:'}" />
