<UserControl
    x:Class="RD3.Views.IndexView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cv="clr-namespace:RD3.Common.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ex="clr-namespace:LayUI.Wpf.Extensions;assembly=LayUI.Wpf.Extensions"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RD3.Views"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:vm="clr-namespace:RD3.ViewModels"
    xmlns:wpf="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
    d:DataContext="{d:DesignInstance Type=vm:IndexViewModel}"
    d:DesignHeight="900"
    d:DesignWidth="1550"
    Background="{StaticResource BackGroundBrush}"
    mc:Ignorable="d">
    <UserControl.Resources />
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid Grid.ColumnSpan="3">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="auto" />
            </Grid.ColumnDefinitions>
            <Border
                Background="#F5F7FA"
                BorderThickness="0.5"
                CornerRadius="8">
                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="5">
                    <ToggleButton
                        Name="tgbDetails"
                        IsChecked="True"
                        Style="{DynamicResource ToggleButtonCustom}">
                        <hc:StatusSwitchElement.CheckedElement>
                            <Border
                                Width="60"
                                Height="30"
                                Background="{DynamicResource DarkPrimaryBrush}"
                                BorderBrush="{DynamicResource BorderBrush}"
                                BorderThickness="0.5"
                                CornerRadius="4">
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="{ex:Language Key='Details'}" />
                            </Border>
                        </hc:StatusSwitchElement.CheckedElement>
                        <Border
                            Width="60"
                            Height="30"
                            BorderBrush="#F5F7FA"
                            BorderThickness="0"
                            CornerRadius="4">
                            <TextBlock
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="Gray"
                                Text="{ex:Language Key='Details'}" />
                        </Border>
                    </ToggleButton>
                    <ToggleButton Name="tgbTrends" Style="{DynamicResource ToggleButtonCustom}">
                        <hc:StatusSwitchElement.CheckedElement>
                            <Border
                                Width="60"
                                Height="30"
                                Background="{DynamicResource DarkPrimaryBrush}"
                                BorderBrush="{DynamicResource BorderBrush}"
                                BorderThickness="0.5"
                                CornerRadius="4">
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="{ex:Language Key='Trends'}" />
                            </Border>
                        </hc:StatusSwitchElement.CheckedElement>
                        <Border
                            Width="60"
                            Height="30"
                            BorderBrush="#F5F7FA"
                            BorderThickness="0"
                            CornerRadius="4">
                            <TextBlock
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="Gray"
                                Text="{ex:Language Key='Trends'}" />
                        </Border>
                    </ToggleButton>
                    <ToggleButton Name="tgbOverview" Style="{DynamicResource ToggleButtonCustom}">
                        <hc:StatusSwitchElement.CheckedElement>
                            <Border
                                Width="60"
                                Height="30"
                                Background="{DynamicResource DarkPrimaryBrush}"
                                BorderBrush="{DynamicResource BorderBrush}"
                                BorderThickness="0.5"
                                CornerRadius="4"
                                Effect="{StaticResource EffectShadow1}">
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Foreground="White"
                                    Text="{ex:Language Key='Overview'}" />
                            </Border>
                        </hc:StatusSwitchElement.CheckedElement>
                        <Border
                            Width="60"
                            Height="30"
                            BorderBrush="#F5F7FA"
                            BorderThickness="0"
                            CornerRadius="4">
                            <TextBlock
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Foreground="Gray"
                                Text="{ex:Language Key='Overview'}" />
                        </Border>
                    </ToggleButton>
                </hc:UniformSpacingPanel>
            </Border>
            <hc:UniformSpacingPanel Grid.Column="2" Spacing="10">
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryTimeSequence}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding SetPIDCommand}"
                    ToolTip="PID Setting"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryAddTemplate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding PauseCommand}"
                    Content="{ex:Language Key='Save as template'}"
                    ToolTip="Pause" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryPause}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding PauseCommand}"
                    Content="{ex:Language Key='Pause'}"
                    ToolTip="Pause" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometrySample}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding SampleCommand}"
                    Content="{ex:Language Key='Sample'}"
                    ToolTip="Sample" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryAddliquid}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding LogCommand}"
                    Content="{ex:Language Key='Add Liquid'}"
                    ToolTip="AddLiquid" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryInoculate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding InoculateCommand}"
                    Content="{ex:Language Key='Inoculate'}"
                    ToolTip="Inoculate" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryHarvest}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding LogCommand}"
                    Content="{ex:Language Key='Harvest'}"
                    ToolTip="Harvest" />
                <Button
                    Width="80"
                    Height="30"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryStop}"
                    Background="#F56C6C"
                    BorderBrush="#F56C6C"
                    BorderThickness="1"
                    Command="{Binding StopCommand}"
                    Content="{ex:Language Key='Stop'}"
                    Foreground="White"
                    ToolTip="Stop" />
            </hc:UniformSpacingPanel>
        </Grid>
        <TabControl
            Grid.Row="1"
            Grid.ColumnSpan="3"
            Margin="0,20,0,0"
            BorderThickness="0"
            Loaded="TabControl_Loaded"
            Style="{StaticResource TabControlBaseStyle}">
            <TabItem
                Name="tabDetails"
                hc:BorderElement.CornerRadius="8"
                Header="{ex:Language Key='Details'}"
                IsSelected="True">
                <Border Background="#F5F7FA" Style="{StaticResource BorderCard}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="640" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border Margin="10" Style="{StaticResource BorderCard}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="260" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <DockPanel>
                                    <hc:ComboBox
                                        BorderThickness="0"
                                        FontSize="30"
                                        SelectedIndex="0"
                                        Style="{StaticResource ComboBoxExtend}">
                                        <ComboBoxItem Content="G01" />
                                        <ComboBoxItem Content="G02" />
                                        <ComboBoxItem Content="G03" />
                                        <ComboBoxItem Content="G04" />
                                        <ComboBoxItem Content="G05" />
                                        <ComboBoxItem Content="G06" />
                                        <ComboBoxItem Content="G07" />
                                        <ComboBoxItem Content="G08" />
                                    </hc:ComboBox>
                                    <hc:UniformSpacingPanel
                                        DockPanel.Dock="Right"
                                        Orientation="Vertical"
                                        Spacing="5">
                                        <TextBlock
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Foreground="{StaticResource SuccessBrush}"
                                            Text="{ex:Language Key='Running'}"
                                            TextAlignment="Right" />
                                    </hc:UniformSpacingPanel>
                                </DockPanel>
                                <Border
                                    Grid.Row="1"
                                    Margin="5"
                                    Style="{StaticResource BorderCard}">
                                    <DockPanel LastChildFill="True">
                                        <Border
                                            Width="350"
                                            Height="40"
                                            Margin="5"
                                            HorizontalAlignment="Left"
                                            Background="#F5F7FA"
                                            BorderThickness="0.5"
                                            CornerRadius="8"
                                            DockPanel.Dock="Top">
                                            <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="5">
                                                <ToggleButton
                                                    Name="tgbDO"
                                                    IsChecked="True"
                                                    Style="{DynamicResource ToggleButtonCustom}">
                                                    <hc:StatusSwitchElement.CheckedElement>
                                                        <Border
                                                            Width="60"
                                                            Height="30"
                                                            Background="{DynamicResource DarkPrimaryBrush}"
                                                            BorderBrush="{DynamicResource BorderBrush}"
                                                            BorderThickness="0.5"
                                                            CornerRadius="4">
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Foreground="White"
                                                                Text="{ex:Language Key='DO'}" />
                                                        </Border>
                                                    </hc:StatusSwitchElement.CheckedElement>
                                                    <Border
                                                        Width="60"
                                                        Height="30"
                                                        BorderBrush="#F5F7FA"
                                                        BorderThickness="0"
                                                        CornerRadius="4">
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Foreground="Gray"
                                                            Text="{ex:Language Key='DO'}" />
                                                    </Border>
                                                </ToggleButton>
                                                <ToggleButton Name="tgbAgit" Style="{DynamicResource ToggleButtonCustom}">
                                                    <hc:StatusSwitchElement.CheckedElement>
                                                        <Border
                                                            Width="60"
                                                            Height="30"
                                                            Background="{DynamicResource DarkPrimaryBrush}"
                                                            BorderBrush="{DynamicResource BorderBrush}"
                                                            BorderThickness="0.5"
                                                            CornerRadius="4">
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Foreground="White"
                                                                Text="{ex:Language Key='Agit'}" />
                                                        </Border>
                                                    </hc:StatusSwitchElement.CheckedElement>
                                                    <Border
                                                        Width="60"
                                                        Height="30"
                                                        BorderBrush="#F5F7FA"
                                                        BorderThickness="0"
                                                        CornerRadius="4">
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Foreground="Gray"
                                                            Text="{ex:Language Key='Agit'}" />
                                                    </Border>
                                                </ToggleButton>
                                                <ToggleButton Name="tgbAir" Style="{DynamicResource ToggleButtonCustom}">
                                                    <hc:StatusSwitchElement.CheckedElement>
                                                        <Border
                                                            Width="60"
                                                            Height="30"
                                                            Background="{DynamicResource DarkPrimaryBrush}"
                                                            BorderBrush="{DynamicResource BorderBrush}"
                                                            BorderThickness="0.5"
                                                            CornerRadius="4"
                                                            Effect="{StaticResource EffectShadow1}">
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Foreground="White"
                                                                Text="{ex:Language Key='Air'}" />
                                                        </Border>
                                                    </hc:StatusSwitchElement.CheckedElement>
                                                    <Border
                                                        Width="60"
                                                        Height="30"
                                                        BorderBrush="#F5F7FA"
                                                        BorderThickness="0"
                                                        CornerRadius="4">
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Foreground="Gray"
                                                            Text="{ex:Language Key='Air'}" />
                                                    </Border>
                                                </ToggleButton>
                                                <ToggleButton Name="tgbPH" Style="{DynamicResource ToggleButtonCustom}">
                                                    <hc:StatusSwitchElement.CheckedElement>
                                                        <Border
                                                            Width="60"
                                                            Height="30"
                                                            Background="{DynamicResource DarkPrimaryBrush}"
                                                            BorderBrush="{DynamicResource BorderBrush}"
                                                            BorderThickness="0.5"
                                                            CornerRadius="4"
                                                            Effect="{StaticResource EffectShadow1}">
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Foreground="White"
                                                                Text="{ex:Language Key='PH'}" />
                                                        </Border>
                                                    </hc:StatusSwitchElement.CheckedElement>
                                                    <Border
                                                        Width="60"
                                                        Height="30"
                                                        BorderBrush="#F5F7FA"
                                                        BorderThickness="0"
                                                        CornerRadius="4">
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Foreground="Gray"
                                                            Text="{ex:Language Key='PH'}" />
                                                    </Border>
                                                </ToggleButton>
                                                <ToggleButton Name="tgbTemp" Style="{DynamicResource ToggleButtonCustom}">
                                                    <hc:StatusSwitchElement.CheckedElement>
                                                        <Border
                                                            Width="60"
                                                            Height="30"
                                                            Background="{DynamicResource DarkPrimaryBrush}"
                                                            BorderBrush="{DynamicResource BorderBrush}"
                                                            BorderThickness="0.5"
                                                            CornerRadius="4"
                                                            Effect="{StaticResource EffectShadow1}">
                                                            <TextBlock
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Foreground="White"
                                                                Text="{ex:Language Key='Temp'}" />
                                                        </Border>
                                                    </hc:StatusSwitchElement.CheckedElement>
                                                    <Border
                                                        Width="60"
                                                        Height="30"
                                                        BorderBrush="#F5F7FA"
                                                        BorderThickness="0"
                                                        CornerRadius="4">
                                                        <TextBlock
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Foreground="Gray"
                                                            Text="{ex:Language Key='Temp'}" />
                                                    </Border>
                                                </ToggleButton>
                                            </hc:UniformSpacingPanel>
                                        </Border>
                                        <wpf:WpfPlot Name="wpfPlot1" Margin="5" />
                                    </DockPanel>
                                </Border>
                                <Grid Grid.Row="2" Margin="10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="10" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="110" />
                                        <ColumnDefinition Width="70" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="30" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock
                                        Grid.Row="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Reactor name'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="G01"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Reactor ID'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="1001"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Serial number'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="1"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Type'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="Unit"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Status'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="Running"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Batch ID'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="24071101"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Batch name'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="G03"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="8"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Affiliated project'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="8"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="kla"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="9"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Running time'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="9"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="01:02:03"
                                        TextAlignment="Right" />
                                    <Image
                                        Grid.Row="3"
                                        Grid.RowSpan="9"
                                        Grid.Column="2"
                                        Margin="10,0,0,0"
                                        Source="/Images/FermentationTank.png" />
                                </Grid>
                            </Grid>
                        </Border>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="220" />
                                    <RowDefinition Height="220" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="440" />
                                    <ColumnDefinition Width="440" />
                                </Grid.ColumnDefinitions>
                                <Border
                                    Margin="10"
                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                    Style="{StaticResource BorderCard}">
                                    <Grid Margin="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="0" />
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="auto" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30" />
                                            <RowDefinition Height="30" />
                                        </Grid.RowDefinitions>
                                        <Image Grid.RowSpan="2" Source="/Images/Main/Temp.png" />
                                        <TextBlock
                                            Grid.Row="0"
                                            Grid.Column="2"
                                            FontSize="{DynamicResource LargeFontSize}"
                                            Text="{ex:Language Key='Temp'}" />
                                        <hc:UniformSpacingPanel
                                            Grid.Row="1"
                                            Grid.Column="2"
                                            Orientation="Horizontal"
                                            Spacing="5">
                                            <TextBlock FontSize="{DynamicResource HeadFontSize}" Text="37.6" />
                                            <TextBlock
                                                FontSize="{DynamicResource HeadFontSize}"
                                                Foreground="Gray"
                                                Text="℃" />
                                        </hc:UniformSpacingPanel>
                                        <Label
                                            Grid.Column="4"
                                            Width="70"
                                            Height="30"
                                            HorizontalAlignment="Left"
                                            HorizontalContentAlignment="Center"
                                            Background="{DynamicResource SuccessBrush}"
                                            BorderBrush="{DynamicResource SuccessBrush}"
                                            Content="Fixed"
                                            FontSize="10"
                                            Foreground="{DynamicResource DefaultBrush}" />
                                        <TextBlock
                                            Grid.Row="0"
                                            Grid.Column="2"
                                            FontSize="{DynamicResource LargeFontSize}"
                                            Text="{ex:Language Key='Temp'}" />
                                    </Grid>
                                </Border>
                                <Border
                                    Grid.Column="1"
                                    Margin="10"
                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                    Style="{StaticResource BorderCard}">
                                    <Grid Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                        </Grid.RowDefinitions>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="auto" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="30" />
                                            </Grid.RowDefinitions>
                                            <Image Grid.RowSpan="2" Source="/Images/Main/PH.png" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                FontSize="{DynamicResource LargeFontSize}"
                                                Text="{ex:Language Key='PH'}" />
                                            <hc:UniformSpacingPanel
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                <TextBlock FontSize="{DynamicResource HeadFontSize}" Text="7.53" />

                                            </hc:UniformSpacingPanel>
                                            <Label
                                                Grid.Column="4"
                                                Width="70"
                                                Height="30"
                                                HorizontalAlignment="Left"
                                                HorizontalContentAlignment="Center"
                                                Background="{DynamicResource SuccessBrush}"
                                                BorderBrush="{DynamicResource SuccessBrush}"
                                                Content="Fixed"
                                                FontSize="10"
                                                Foreground="{DynamicResource DefaultBrush}" />
                                        </Grid>
                                        <Image
                                            Grid.Row="1"
                                            Width="200"
                                            HorizontalAlignment="Left"
                                            Source="/Images/Main/PH_ass.png" />
                                    </Grid>
                                </Border>
                                <Border
                                    Grid.Row="1"
                                    Margin="10"
                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                    Style="{StaticResource BorderCard}">
                                    <Grid Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                        </Grid.RowDefinitions>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="auto" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="30" />
                                            </Grid.RowDefinitions>
                                            <Image Grid.RowSpan="2" Source="/Images/Main/AGit.png" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                FontSize="{DynamicResource LargeFontSize}"
                                                Text="{ex:Language Key='Agit'}" />
                                            <hc:UniformSpacingPanel
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                <TextBlock FontSize="{DynamicResource HeadFontSize}" Text="1000" />
                                                <TextBlock
                                                    FontSize="{DynamicResource HeadFontSize}"
                                                    Foreground="Gray"
                                                    Text="RPM" />
                                            </hc:UniformSpacingPanel>
                                            <Label
                                                Grid.Column="4"
                                                Width="70"
                                                Height="30"
                                                HorizontalAlignment="Left"
                                                HorizontalContentAlignment="Center"
                                                Background="{DynamicResource SuccessBrush}"
                                                BorderBrush="{DynamicResource SuccessBrush}"
                                                Content="Fixed"
                                                FontSize="10"
                                                Foreground="{DynamicResource DefaultBrush}" />
                                        </Grid>
                                        <Image
                                            Grid.Row="1"
                                            Grid.ColumnSpan="3"
                                            Width="200"
                                            HorizontalAlignment="Left"
