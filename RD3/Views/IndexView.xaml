<UserControl
    x:Class="RD3.Views.IndexView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behavior="clr-namespace:RD3.Behaviors"
    xmlns:common="clr-namespace:RD3.Common"
    xmlns:cv="clr-namespace:RD3.Common.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ex="clr-namespace:LayUI.Wpf.Extensions;assembly=LayUI.Wpf.Extensions"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RD3.Views"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:share="clr-namespace:RD3.Shared;assembly=RD3.Shared"
    xmlns:vm="clr-namespace:RD3.ViewModels"
    xmlns:wpf="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
    d:DataContext="{d:DesignInstance Type=vm:IndexViewModel}"
    d:DesignHeight="950"
    d:DesignWidth="1550"
    Background="{StaticResource BackGroundBrush}"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Style BasedOn="{StaticResource CheckBoxBaseStyle}" TargetType="CheckBox">
            <Setter Property="Margin" Value="8" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
        </Style>
    </UserControl.Resources>
    <UserControl.Triggers>
        <EventTrigger RoutedEvent="UserControl.Loaded">
            <BeginStoryboard>
                <Storyboard>
                    <!--<DoubleAnimation
                        BeginTime="0:0:3"
                        FillBehavior="HoldEnd"
                        Storyboard.Target="{StaticResource Progress1}"
                        Storyboard.TargetProperty="Progress"
                        To="50"
                        Duration="0:0:10" />-->
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>
    </UserControl.Triggers>
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <materialDesign:Snackbar
            x:Name="LoginSnakeBar"
            Grid.Row="1"
            Grid.ColumnSpan="2"
            Width="200"
            Height="100"
            Panel.ZIndex="1"
            MessageQueue="{materialDesign:MessageQueue}" />
        <Grid Grid.ColumnSpan="3">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="auto" />
            </Grid.ColumnDefinitions>
            <Border
                Height="50"
                Background="{DynamicResource BorderBrush}"
                BorderThickness="0.5"
                CornerRadius="8">
                <hc:ButtonGroup
                    Name="ButtonGroup"
                    VerticalAlignment="Center"
                    Style="{StaticResource ButtonGroupSolid}">
                    <RadioButton
                        Name="RadioButtonDetail"
                        Width="100"
                        Height="40"
                        Margin="5,0,0,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Detail'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        IsChecked="True"
                        Tag="tabDetail" />
                    <RadioButton
                        Name="RadioButtonTrend"
                        Width="100"
                        Height="40"
                        Margin="5,0,5,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Trend'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        Tag="tabTrend" />
                    <RadioButton
                        Name="RadioButtonOverview"
                        Width="100"
                        Height="40"
                        Margin="5,0,5,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Overview'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        Tag="tabOverview" />
                </hc:ButtonGroup>
            </Border>
            <hc:UniformSpacingPanel Grid.Column="2" Spacing="5">
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryLoadSetting}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding LoadSettingCommand}"
                    Content="{ex:Language Key='Load Setting'}"
                    ToolTip="Load Setting"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryImportTemplate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding ImportTemplateCommand}"
                    Content="{ex:Language Key='Import template'}"
                    ToolTip="Import template"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryAddTemplate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding SaveAsTemplateCommand}"
                    Content="{ex:Language Key='Save as template'}"
                    ToolTip="Save as template" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryPause}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding PauseCommand}"
                    Content="{ex:Language Key='Pause'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="Pause" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometrySample}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding UnScheduleCommand}"
                    CommandParameter="Sample"
                    Content="{ex:Language Key='Sample'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="Sample" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryAddliquid}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding UnScheduleCommand}"
                    CommandParameter="Addliquid"
                    Content="{ex:Language Key='Add Liquid'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="AddLiquid" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryInoculate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding UnScheduleCommand}"
                    CommandParameter="Inoculate"
                    Content="{ex:Language Key='Inoculate'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="Inoculate" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryHarvest}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding UnScheduleCommand}"
                    CommandParameter="Harvest"
                    Content="{ex:Language Key='Harvest'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="Harvest" />
                <Button
                    Width="80"
                    Height="30"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryStop}"
                    Background="{DynamicResource StopBrush}"
                    BorderBrush="{DynamicResource StopBrush}"
                    BorderThickness="1"
                    Command="{Binding StopCommand}"
                    Content="{ex:Language Key='Stop'}"
                    Foreground="White"
                    ToolTip="Stop"
                    Visibility="{Binding Running, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Button
                    Name="ButtonStart"
                    Width="80"
                    Height="30"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryStart}"
                    Background="{DynamicResource PrimaryBrush}"
                    BorderBrush="{DynamicResource PrimaryBrush}"
                    BorderThickness="1"
                    Click="ButtonStart_Click"
                    Content="{ex:Language Key='Start'}"
                    Foreground="White"
                    ToolTip="Start"
                    Visibility="{Binding Running, Converter={StaticResource InverseBooleanToVisibilityConverter}}" />
            </hc:UniformSpacingPanel>
        </Grid>
        <TabControl
            Name="tabControl"
            Grid.Row="1"
            Grid.ColumnSpan="3"
            Margin="0,20,0,0"
            BorderThickness="0"
            Loaded="TabControl_Loaded"
            Style="{StaticResource TabControlBaseStyle}">
            <TabItem
                Name="tabDetail"
                hc:BorderElement.CornerRadius="8"
                Header="{ex:Language Key='Detail'}"
                IsSelected="True">
                <Border Background="#F5F7FA" Style="{StaticResource BorderCard}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="640" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border Margin="10" Style="{StaticResource BorderCard}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="300" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <DockPanel>
                                    <hc:ComboBox
                                        Name="CmbDevice"
                                        BorderThickness="0"
                                        FontSize="{DynamicResource LargeFontSize}"
                                        SelectedIndex="0"
                                        Style="{StaticResource ComboBoxExtend}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="SelectionChanged">
                                                <i:InvokeCommandAction
                                                    Command="{Binding ChangeDeviceCommand}"
                                                    CommandParameter="{Binding ElementName=CmbDevice, Path=SelectedIndex}"
                                                    PassEventArgsToCommand="True" />
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                        <ComboBoxItem Content="G01" />
                                        <ComboBoxItem Content="G02" />
                                        <ComboBoxItem Content="G03" />
                                        <ComboBoxItem Content="G04" />
                                        <ComboBoxItem Content="G05" />
                                        <ComboBoxItem Content="G06" />
                                        <ComboBoxItem Content="G07" />
                                        <ComboBoxItem Content="G08" />
                                    </hc:ComboBox>
                                    <hc:UniformSpacingPanel
                                        DockPanel.Dock="Right"
                                        Orientation="Vertical"
                                        Spacing="5">
                                        <TextBlock
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Foreground="{StaticResource SuccessBrush}"
                                            Text="{ex:Language Key='Running'}"
                                            TextAlignment="Right"
                                            Visibility="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Running}" />
                                        <TextBlock
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Foreground="{StaticResource SuccessBrush}"
                                            Text="{ex:Language Key='Idle'}"
                                            TextAlignment="Right"
                                            Visibility="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Idle}" />
                                    </hc:UniformSpacingPanel>
                                </DockPanel>
                                <Border
                                    Grid.Row="1"
                                    Margin="5"
                                    Style="{StaticResource BorderCard}">
                                    <DockPanel LastChildFill="True">
                                        <Border
                                            Height="40"
                                            HorizontalAlignment="Left"
                                            Background="{DynamicResource BorderBrush}"
                                            BorderThickness="0.5"
                                            CornerRadius="8"
                                            DockPanel.Dock="Top">
                                            <hc:ButtonGroup
                                                Name="ButtonGroup1"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource ButtonGroupSolid}">
                                                <RadioButton
                                                    Width="60"
                                                    Height="30"
                                                    Margin="5,0,0,0"
                                                    hc:BorderElement.CornerRadius="8"
                                                    hc:VisualElement.HighlightBackground="{DynamicResource PrimaryBrush}"
                                                    hc:VisualElement.HighlightBorderBrush="{DynamicResource PrimaryBrush}"
                                                    hc:VisualElement.HighlightForeground="{DynamicResource TextIconBrush}"
                                                    Background="{DynamicResource BorderBrush}"
                                                    Content="{ex:Language Key='DO'}"
                                                    FontSize="{DynamicResource TextFontSize}"
                                                    Foreground="{DynamicResource SecondaryTextBrush}"
                                                    IsChecked="{Binding CurrentExperimentParameter, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=DO}" />
                                                <RadioButton
                                                    Width="60"
                                                    Height="30"
                                                    Margin="5,0,0,0"
                                                    hc:BorderElement.CornerRadius="8"
                                                    Background="{DynamicResource BorderBrush}"
                                                    Content="{ex:Language Key='Agit'}"
                                                    FontSize="{DynamicResource TextFontSize}"
                                                    Foreground="{DynamicResource SecondaryTextBrush}"
                                                    IsChecked="{Binding CurrentExperimentParameter, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Agit}" />
                                                <RadioButton
                                                    Width="60"
                                                    Height="30"
                                                    Margin="5,0,0,0"
                                                    hc:BorderElement.CornerRadius="8"
                                                    Background="{DynamicResource BorderBrush}"
                                                    Content="{ex:Language Key='Air'}"
                                                    FontSize="{DynamicResource TextFontSize}"
                                                    Foreground="{DynamicResource SecondaryTextBrush}"
                                                    IsChecked="{Binding CurrentExperimentParameter, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Air}" />
                                                <RadioButton
                                                    Width="60"
                                                    Height="30"
                                                    Margin="5,0,0,0"
                                                    hc:BorderElement.CornerRadius="8"
                                                    Background="{DynamicResource BorderBrush}"
                                                    Content="{ex:Language Key='PH'}"
                                                    FontSize="{DynamicResource TextFontSize}"
                                                    Foreground="{DynamicResource SecondaryTextBrush}"
                                                    IsChecked="{Binding CurrentExperimentParameter, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=PH}" />
                                                <RadioButton
                                                    Width="60"
                                                    Height="30"
                                                    Margin="5,0,0,0"
                                                    hc:BorderElement.CornerRadius="8"
                                                    Background="{DynamicResource BorderBrush}"
                                                    Content="{ex:Language Key='Temp'}"
                                                    FontSize="{DynamicResource TextFontSize}"
                                                    Foreground="{DynamicResource SecondaryTextBrush}"
                                                    IsChecked="{Binding CurrentExperimentParameter, Converter={StaticResource EnumToBoolConverter}, ConverterParameter=Temp}" />
                                            </hc:ButtonGroup>
                                        </Border>
                                        <wpf:WpfPlot Name="wpfPlot1" />
                                    </DockPanel>
                                </Border>
                                <Grid Grid.Row="2" Margin="10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="0" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="20" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="30" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock
                                        Grid.Row="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Reactor name'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentDeviceParameter.Name}"
                                        TextAlignment="Right" />
                                    <!--<TextBlock
                                        Grid.Row="2"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Reactor ID'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentDeviceParameter.Id}"
                                        TextAlignment="Right" />-->
                                    <TextBlock
                                        Grid.Row="3"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Serial number'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentDeviceParameter.SerialNumber}"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Type'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="Unit"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Status'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToStringConverter}}"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Batch ID'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentBatch.Id}"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Batch name'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentBatch.Name}"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="8"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Affiliated project'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="8"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding CurrentBatch.Project}"
                                        TextAlignment="Right" />
                                    <TextBlock
                                        Grid.Row="9"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{ex:Language Key='Running time'}"
                                        TextAlignment="Left" />
                                    <TextBlock
                                        Grid.Row="9"
                                        Grid.Column="1"
                                        Style="{StaticResource BodyFontSizeStyle}"
                                        Text="{Binding FormattedTime}"
                                        TextAlignment="Right" />
                                    <Image
                                        Grid.Row="3"
                                        Grid.RowSpan="9"
                                        Grid.Column="2"
                                        Margin="10,0,0,0"
                                        Source="/Images/FermentationTank.png" />
                                </Grid>
                            </Grid>
                        </Border>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="220" />
                                    <RowDefinition Height="220" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="440" />
                                    <ColumnDefinition Width="440" />
                                </Grid.ColumnDefinitions>
                                <Border
                                    Margin="10"
                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                    Style="{StaticResource BorderCard}">
                                    <Grid Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="25" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="auto" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="30" />
                                            </Grid.RowDefinitions>
                                            <Image Grid.RowSpan="2" Source="/Images/Main/Temp_Primary.png" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                FontSize="{DynamicResource LargeFontSize}"
                                                Text="{ex:Language Key='Temp'}" />
                                            <hc:UniformSpacingPanel
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                <TextBlock FontSize="{DynamicResource HeadFontSize}" Text="37.6" />
                                                <TextBlock
                                                    FontSize="{DynamicResource HeadFontSize}"
                                                    Foreground="Gray"
                                                    Text="℃" />
                                            </hc:UniformSpacingPanel>
                                            <hc:UniformSpacingPanel Grid.Column="4" Orientation="Horizontal">
                                                <Label
                                                    Width="70"
                                                    Height="30"
                                                    HorizontalAlignment="Left"
                                                    HorizontalContentAlignment="Center"
                                                    Background="{DynamicResource SuccessBrush}"
                                                    BorderBrush="{DynamicResource SuccessBrush}"
                                                    Content="Fixed"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource DefaultBrush}"
                                                    Visibility="{Binding CurrentDeviceParameter.TempParam.ControlMode, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Fixed}" />
                                                <Label
                                                    Width="80"
                                                    Height="30"
                                                    HorizontalAlignment="Left"
                                                    HorizontalContentAlignment="Center"
                                                    Background="{DynamicResource PrimaryBrush}"
                                                    BorderBrush="{DynamicResource PrimaryBrush}"
                                                    Content="Associated"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource DefaultBrush}"
                                                    Visibility="{Binding CurrentDeviceParameter.TempParam.ControlMode, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Associated}" />
                                                <Label
                                                    Width="70"
                                                    Height="30"
                                                    HorizontalAlignment="Left"
                                                    HorizontalContentAlignment="Center"
                                                    Background="#3C739A"
                                                    BorderBrush="#3C739A"
                                                    Content="Free"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource DefaultBrush}"
                                                    Visibility="{Binding CurrentDeviceParameter.TempParam.ControlMode, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Free}" />
                                                <Label
                                                    Width="70"
                                                    Height="30"
                                                    HorizontalAlignment="Left"
                                                    HorizontalContentAlignment="Center"
                                                    Background="{DynamicResource ThirdlyTextBrush}"
                                                    BorderBrush="{DynamicResource ThirdlyTextBrush}"
                                                    Content="Disable"
                                                    FontSize="10"
                                                    Foreground="{DynamicResource DefaultBrush}"
                                                    Visibility="{Binding CurrentDeviceParameter.TempParam.ControlMode, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Disable}" />
                                            </hc:UniformSpacingPanel>
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                FontSize="{DynamicResource LargeFontSize}"
                                                Text="{ex:Language Key='Temp'}" />
                                        </Grid>
                                        <Grid
                                            Grid.Row="2"
                                            Width="200"
                                            Height="200">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="1.4*" />
                                                <RowDefinition />
                                                <RowDefinition />
                                            </Grid.RowDefinitions>
                                            <Ellipse
                                                Grid.RowSpan="3"
                                                Margin="4"
                                                Stroke="{DynamicResource BorderBrush}"
                                                StrokeDashCap="Round"
                                                StrokeThickness="4">
                                                <i:Interaction.Behaviors>
                                                    <behavior:EllipseProgressBehavior
                                                        EndAngle="100"
                                                        Progress="100"
                                                        StartAngle="-100" />
                                                </i:Interaction.Behaviors>
                                            </Ellipse>
                                            <Ellipse
                                                Grid.RowSpan="3"
                                                Stroke="{DynamicResource PrimaryBrush}"
                                                StrokeDashCap="Round"
                                                StrokeThickness="12">
                                                <i:Interaction.Behaviors>
                                                    <behavior:EllipseProgressBehavior
                                                        EndAngle="100"
                                                        Progress="{Binding Progress1}"
                                                        StartAngle="-100" />
                                                </i:Interaction.Behaviors>
                                            </Ellipse>
                                            <hc:UniformSpacingPanel
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Bottom"
                                                Orientation="Vertical"
                                                Spacing="10">
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    FontSize="{DynamicResource HeadFontSize}"
                                                    Foreground="{DynamicResource PrimaryBrush}"
                                                    Text="{Binding CurrentDeviceParameter.Temp, StringFormat=N2}"
                                                    TextAlignment="Center" />
                                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="5">
                                                    <TextBlock Text="{ex:Language Key='Target Value:'}" />
                                                    <TextBlock Text="{Binding CurrentDeviceParameter.TempParam.Temp_PV, StringFormat=N2}" />
                                                </hc:UniformSpacingPanel>
                                            </hc:UniformSpacingPanel>
                                        </Grid>
                                    </Grid>
                                </Border>
                                <Border
                                    Grid.Column="1"
                                    Margin="10"
                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                    Style="{StaticResource BorderCard}">
                                    <Grid Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="auto" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="30" />
                                            </Grid.RowDefinitions>
                                            <Image Grid.RowSpan="2" Source="/Images/Main/PH_Primary.png" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                FontSize="{DynamicResource LargeFontSize}"
                                                Text="{ex:Language Key='PH'}" />
                                            <hc:UniformSpacingPanel
                                                Grid.Row="1"
                                                Grid.Column="2"
