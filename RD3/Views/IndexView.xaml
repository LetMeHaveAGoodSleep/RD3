<UserControl
    x:Class="RD3.Views.IndexView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behavior="clr-namespace:RD3.Behaviors"
    xmlns:common="clr-namespace:RD3.Common"
    xmlns:cv="clr-namespace:RD3.Common.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ex="clr-namespace:LayUI.Wpf.Extensions;assembly=LayUI.Wpf.Extensions"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:RD3.Views"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:share="clr-namespace:RD3.Shared;assembly=RD3.Shared"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:vm="clr-namespace:RD3.ViewModels"
    xmlns:wpf="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
    d:DataContext="{d:DesignInstance Type=vm:IndexViewModel}"
    d:DesignHeight="910"
    d:DesignWidth="1385"
    Background="{StaticResource BackGroundBrush}"
    TextElement.FontFamily="{DynamicResource AppFontFamily}"
    TextElement.FontSize="{DynamicResource TextFontSize}"
    TextElement.FontWeight="Regular"
    TextElement.Foreground="{DynamicResource MaterialDesignBody}"
    TextOptions.TextFormattingMode="Ideal"
    TextOptions.TextRenderingMode="Auto"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Style BasedOn="{StaticResource CheckBoxBaseStyle}" TargetType="CheckBox">
            <Setter Property="Margin" Value="8" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
        </Style>
        <ObjectDataProvider
            x:Key="DataFromEnum"
            MethodName="GetValues"
            ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="share:ExperimentParameter" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </UserControl.Resources>
    <UserControl.Triggers>
        <EventTrigger RoutedEvent="UserControl.Loaded">
            <BeginStoryboard>
                <Storyboard />
            </BeginStoryboard>
        </EventTrigger>
    </UserControl.Triggers>
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <materialDesign:Snackbar
            x:Name="LoginSnakeBar"
            Grid.Row="1"
            Grid.ColumnSpan="2"
            Width="350"
            Height="100"
            HorizontalAlignment="Center"
            VerticalAlignment="Bottom"
            Panel.ZIndex="1"
            MessageQueue="{materialDesign:MessageQueue}" />
        <DockPanel Grid.ColumnSpan="3" LastChildFill="True">
            <Border
                Height="40"
                Background="{DynamicResource BorderBrush}"
                BorderThickness="0.5"
                CornerRadius="8"
                DockPanel.Dock="Left">
                <hc:ButtonGroup
                    Name="ButtonGroup"
                    VerticalAlignment="Center"
                    Style="{StaticResource ButtonGroupSolid}">
                    <RadioButton
                        Name="RadioButtonDetail"
                        Width="100"
                        Height="40"
                        Margin="5,0,0,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Detail'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        IsChecked="True"
                        Tag="tabDetail" />
                    <RadioButton
                        Name="RadioButtonTrend"
                        Width="100"
                        Height="40"
                        Margin="5,0,5,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Trend'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        Tag="tabTrend"
                        Visibility="Collapsed" />
                    <RadioButton
                        Name="RadioButtonOverview"
                        Width="100"
                        Height="40"
                        Margin="5,0,5,0"
                        hc:BorderElement.CornerRadius="8"
                        Background="{DynamicResource BorderBrush}"
                        Content="{ex:Language Key='Overview'}"
                        FontSize="{DynamicResource MediumFontSize}"
                        Foreground="{DynamicResource SecondaryTextBrush}"
                        Tag="tabOverview" />
                </hc:ButtonGroup>
            </Border>
            <hc:UniformSpacingPanel DockPanel.Dock="Right" Spacing="5">
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryLoadSetting}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding LoadSettingCommand}"
                    Content="{ex:Language Key='Load Setting'}"
                    ToolTip="Load Setting"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryImport}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding ImportHistoryDataCommand}"
                    Content="{ex:Language Key='Import history data'}"
                    ToolTip="Import template"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource SaveGeometry}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding SaveDataCommand}"
                    Content="{ex:Language Key='Save data'}"
                    ToolTip="Save as template"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometryImportTemplate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding ImportTemplateCommand}"
                    Content="{ex:Language Key='Import template'}"
                    ToolTip="Import template"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryAddTemplate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding SaveAsTemplateCommand}"
                    Content="{ex:Language Key='Save as template'}"
                    ToolTip="Save as template"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryPause}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding PauseCommand}"
                    Content="{ex:Language Key='Pause'}"
                    IsEnabled="{Binding Running}"
                    ToolTip="Pause"
                    Visibility="Collapsed" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryCamera}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding CameraCommand}"
                    Content="{ex:Language Key='实时观测'}" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometrySample}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding TestCommand}"
                    CommandParameter="Sample"
                    Content="{ex:Language Key='Sample'}"
                    ToolTip="取样" />
                <Button
                    Height="30"
                    HorizontalAlignment="Left"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryInoculate}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{Binding UnScheduleCommand}"
                    CommandParameter="Inoculate"
                    Content="{ex:Language Key='Inoculate'}"
                    ToolTip="接种" />
                <Button
                    Width="100"
                    Height="30"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryRelease}"
                    Background="{DynamicResource StopBrush}"
                    BorderBrush="{DynamicResource StopBrush}"
                    BorderThickness="1"
                    Command="{Binding ReleaseCommand}"
                    Content="{ex:Language Key='一键下罐'}"
                    Foreground="White"
                    IsEnabled="{Binding StopFlag, Converter={StaticResource InverseBooleanConverter}}" />
                <Button
                    Width="80"
                    Height="30"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryStop}"
                    Background="{DynamicResource StopBrush}"
                    BorderBrush="{DynamicResource StopBrush}"
                    BorderThickness="1"
                    Command="{Binding StopCommand}"
                    Content="{ex:Language Key='Stop'}"
                    Foreground="White"
                    ToolTip="Stop"
                    Visibility="{Binding Running, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Button
                    Name="ButtonStart"
                    Width="80"
                    Height="30"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    hc:IconElement.Geometry="{StaticResource GeometryStart}"
                    Background="{DynamicResource PrimaryBrush}"
                    BorderBrush="{DynamicResource PrimaryBrush}"
                    BorderThickness="1"
                    Click="ButtonStart_Click"
                    Content="{ex:Language Key='Start'}"
                    Foreground="White"
                    ToolTip="Start"
                    Visibility="{Binding Running, Converter={StaticResource InverseBooleanToVisibilityConverter}}" />
            </hc:UniformSpacingPanel>
            <Grid Margin="5,0,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <hc:UniformSpacingPanel VerticalAlignment="Center" Spacing="5">
                    <TextBlock FontSize="{DynamicResource SubHeadFontSize}" Text="{ex:Language Key='Runnning time:'}" />
                    <TextBlock
                        HorizontalAlignment="Right"
                        FontSize="{DynamicResource SubHeadFontSize}"
                        Foreground="Green"
                        Text="{Binding FormattedTime}" />
                </hc:UniformSpacingPanel>
                <Border
                    Name="BorderDevice"
                    Grid.Column="1"
                    Margin="5,0,5,0"
                    Background="White"
                    BorderBrush="LightGray"
                    BorderThickness="1"
                    CornerRadius="8">
                    <hc:ScrollViewer
                        hc:ScrollViewerAttach.AutoHide="True"
                        hc:ScrollViewerAttach.Orientation="Vertical"
                        HorizontalScrollBarVisibility="Disabled"
                        VerticalScrollBarVisibility="Auto">
                        <WrapPanel
                            Name="ChkList"
                            Orientation="Horizontal"
                            ScrollViewer.CanContentScroll="True">
                            <CheckBox Name="ChkAll" Content="Select all" />
                        </WrapPanel>
                    </hc:ScrollViewer>
                </Border>
                <Button
                    Grid.Column="2"
                    Height="35"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    hc:BorderElement.CornerRadius="8"
                    hc:IconElement.Geometry="{StaticResource GeometrySetting}"
                    Background="Transparent"
                    BorderThickness="1"
                    Command="{x:Static materialDesign:DrawerHost.OpenDrawerCommand}"
                    CommandParameter="{x:Static Dock.Right}"
                    Content="{ex:Language Key='图表设置'}"
                    Visibility="Collapsed" />
            </Grid>
        </DockPanel>
        <TabControl
            Name="tabControl"
            Grid.Row="1"
            Grid.ColumnSpan="3"
            Margin="0,5,0,0"
            BorderThickness="0"
            Loaded="TabControl_Loaded"
            Style="{StaticResource TabControlBaseStyle}">
            <TabItem
                Name="tabDetail"
                hc:BorderElement.CornerRadius="8"
                Header="{ex:Language Key='Detail'}"
                IsSelected="True">
                <materialDesign:DrawerHost
                    x:Name="DrawerHost"
                    BorderThickness="0"
                    OpenMode="Modal"
                    OverlayBackground="{DynamicResource PrimaryBrush}"
                    RightDrawerCornerRadius="8">
                    <materialDesign:DrawerHost.Style>
                        <Style BasedOn="{StaticResource {x:Type materialDesign:DrawerHost}}" TargetType="materialDesign:DrawerHost" />
                    </materialDesign:DrawerHost.Style>
                    <materialDesign:DrawerHost.RightDrawerContent>
                        <Grid Width="250" Margin="10">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="20" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                FontSize="16"
                                FontWeight="Bold"
                                Text="{ex:Language Key='图表设置'}" />
                            <Grid Grid.Row="2" Margin="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock VerticalAlignment="Center" Text="{ex:Language Key='Grid line'}" />
                                <ToggleButton
                                    Grid.Column="2"
                                    Checked="ToggleButtonGraphSetting_CheckChanged"
                                    IsChecked="True"
                                    Style="{StaticResource ToggleButtonSwitch}"
                                    Unchecked="ToggleButtonGraphSetting_CheckChanged" />
                            </Grid>
                            <Line
                                Grid.Row="3"
                                HorizontalAlignment="Left"
                                Stroke="LightGray"
                                StrokeDashArray="2 2"
                                StrokeThickness="1"
                                Visibility="Collapsed"
                                X1="10"
                                X2="240"
                                Y1="0"
                                Y2="0" />
                            <Grid
                                Grid.Row="4"
                                Margin="5"
                                Visibility="Collapsed">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock VerticalAlignment="Center" Text="{ex:Language Key='Axis name'}" />
                                <hc:ComboBox
                                    Name="CmbYAxis"
                                    Grid.Column="2"
                                    Width="80"
                                    VerticalContentAlignment="Center"
                                    hc:BorderElement.CornerRadius="4"
                                    ItemsSource="{Binding YAxisSource}"
                                    SelectionChanged="CmbYAxis_SelectionChanged" />
                            </Grid>
                            <Line
                                Grid.Row="5"
                                HorizontalAlignment="Left"
                                Stroke="LightGray"
                                StrokeDashArray="2 2"
                                StrokeThickness="1"
                                Visibility="Collapsed"
                                X1="10"
                                X2="240"
                                Y1="0"
                                Y2="0" />
                            <Grid
                                Grid.Row="6"
                                Margin="5"
                                Visibility="Collapsed">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock VerticalAlignment="Center" Text="{ex:Language Key='Auto Scale'}" />
                                <ToggleButton
                                    Name="ToggleAutoScale"
                                    Grid.Column="2"
                                    Checked="ToggleButtonGraphSetting_CheckChanged"
                                    IsChecked="False"
                                    Style="{StaticResource ToggleButtonSwitch}"
                                    Unchecked="ToggleButtonGraphSetting_CheckChanged" />
                            </Grid>
                            <Line
                                Grid.Row="7"
                                HorizontalAlignment="Left"
                                Stroke="LightGray"
                                StrokeDashArray="2 2"
                                StrokeThickness="1"
                                Visibility="Collapsed"
                                X1="10"
                                X2="240"
                                Y1="0"
                                Y2="0" />
                            <Grid
                                Grid.Row="8"
                                Margin="5"
                                Visibility="Collapsed">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <TextBlock VerticalAlignment="Center" Text="{ex:Language Key='Scale Range'}" />
                                <TextBox
                                    Name="TxtMinScale"
                                    Grid.Column="2"
                                    Width="50"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBoxExtend}" />
                                <TextBox
                                    Name="TxtMaxScale"
                                    Grid.Column="4"
                                    Width="50"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBoxExtend}" />
                                <Button
                                    Name="BtnScale"
                                    Grid.Column="6"
                                    Click="BtnScale_Click"
                                    Content="{ex:Language Key='OK'}" />
                            </Grid>
                        </Grid>
                    </materialDesign:DrawerHost.RightDrawerContent>
                    <Border
                        Name="BorderMain"
                        Background="{StaticResource BackGroundBrush}"
                        Style="{StaticResource BorderCard}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Border
                                    Name="BorderReactor"
                                    Background="{StaticResource BackGroundBrush}"
                                    Style="{StaticResource BorderCard}">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="5" />
                                            <RowDefinition Height="auto" />
                                        </Grid.RowDefinitions>
                                        <DockPanel>
                                            <hc:ComboBox
                                                Name="CmbDevice"
                                                Height="30"
                                                BorderThickness="0"
                                                FontSize="{DynamicResource SubHeadFontSize}"
                                                ItemsSource="{Binding ReactorCol}"
                                                SelectedIndex="0"
                                                Style="{StaticResource ComboBoxExtend}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged">
                                                        <i:InvokeCommandAction
                                                            Command="{Binding ChangeDeviceCommand}"
                                                            CommandParameter="{Binding ElementName=CmbDevice, Path=SelectedIndex}"
                                                            PassEventArgsToCommand="True" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                                <ComboBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Name}" />
                                                    </DataTemplate>
                                                </ComboBox.ItemTemplate>
                                            </hc:ComboBox>
                                            <hc:UniformSpacingPanel
                                                DockPanel.Dock="Right"
                                                Orientation="Vertical"
                                                Spacing="5">
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontSize="{StaticResource SubHeadFontSize}"
                                                    FontWeight="Bold"
                                                    Foreground="{StaticResource FixedBrush}"
                                                    Text="{ex:Language Key='Running'}"
                                                    TextAlignment="Right"
                                                    Visibility="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Running}" />
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontSize="{StaticResource SubHeadFontSize}"
                                                    FontWeight="Bold"
                                                    Foreground="{StaticResource WarningBrush}"
                                                    Text="{ex:Language Key='空闲'}"
                                                    TextAlignment="Right"
                                                    Visibility="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Idle}" />
                                                <TextBlock
                                                    VerticalAlignment="Center"
                                                    FontSize="{StaticResource SubHeadFontSize}"
                                                    FontWeight="Bold"
                                                    Foreground="{StaticResource WarningBrush}"
                                                    Text="{ex:Language Key='空闲'}"
                                                    TextAlignment="Right"
                                                    Visibility="{Binding CurrentDeviceParameter.WorkStatus, Converter={StaticResource EnumToVisibilityConverter}, ConverterParameter=Initializing}" />
                                            </hc:UniformSpacingPanel>
                                        </DockPanel>
                                        <Border
                                            Grid.Row="1"
                                            Width="950"
                                            Margin="5"
                                            Style="{StaticResource BorderCard}">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition Width="5" />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition
                                                            Name="rowGraph1"
                                                            Height="auto"
                                                            MaxHeight="60" />
                                                        <RowDefinition Height="5" />
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>
                                                    <ScrollViewer HorizontalScrollBarVisibility="Auto">
                                                        <WrapPanel Name="ChkGraphList1" Orientation="Horizontal" />
                                                    </ScrollViewer>
                                                    <wpf:WpfPlot Name="wpfPlot1" Grid.Row="2" />
                                                </Grid>
                                                <Grid Grid.Column="2">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition
                                                            Name="rowGraph2"
                                                            Height="auto"
                                                            MaxHeight="60" />
                                                        <RowDefinition Height="5" />
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <ScrollViewer HorizontalScrollBarVisibility="Auto">
                                                            <WrapPanel Name="ChkGraphList2" Orientation="Horizontal" />
                                                        </ScrollViewer>
                                                    </Grid>
                                                    <wpf:WpfPlot Name="wpfPlot2" Grid.Row="2" />
                                                </Grid>
                                            </Grid>
                                        </Border>
                                        <GridSplitter
                                            Grid.Row="2"
                                            Height="5"
                                            HorizontalAlignment="Stretch"
                                            Background="LightGray" />
                                        <Grid Grid.Row="3" Margin="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="30" />
                                                <ColumnDefinition Width="auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="auto" />
                                            </Grid.ColumnDefinitions>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="auto" />
                                                    <ColumnDefinition Width="5" />
                                                    <ColumnDefinition Width="auto" />
                                                    <ColumnDefinition Width="5" />
                                                    <ColumnDefinition Width="auto" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                    <RowDefinition Height="8" />
                                                    <RowDefinition Height="auto" />
                                                </Grid.RowDefinitions>
                                                <TextBlock
                                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                                    Text="温度（℃）"
                                                    TextAlignment="Left" />
                                                <hc:UniformSpacingPanel
                                                    Grid.Column="2"
                                                    Orientation="Horizontal"
                                                    Spacing="5">
                                                    <TextBlock
                                                        Width="45"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding CurrentDeviceParameter.Temp, StringFormat={}{0:F2}}"
                                                        TextTrimming="WordEllipsis"
                                                        ToolTip="{Binding CurrentDeviceParameter.Temp, StringFormat={}{0:F2}}" />
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="/" />
                                                    <TextBlock
                                                        Width="45"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding CurrentDeviceParameter.TempParam.Temp_PV, StringFormat={}{0:F2}}"
                                                        TextTrimming="WordEllipsis"
                                                        ToolTip="{Binding CurrentDeviceParameter.TempParam.Temp_PV, StringFormat={}{0:F2}}" />
                                                </hc:UniformSpacingPanel>
                                                <ToggleButton
                                                    Grid.Column="4"
                                                    HorizontalAlignment="Center"
                                                    Background="Gray"
                                                    BorderThickness="1"
                                                    Checked="ToggleButton_CheckStateChanged"
                                                    IsChecked="{Binding CurrentDeviceParameter.TempEnable, Mode=OneWay}"
                                                    Style="{StaticResource ToggleButtonSwitch}"
                                                    Tag="Temperature"
                                                    Unchecked="ToggleButton_CheckStateChanged" />
                                                <TextBlock
                                                    Grid.Row="2"
                                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                                    Text="DO（%）"
                                                    TextAlignment="Left" />
                                                <hc:UniformSpacingPanel
                                                    Grid.Row="2"
                                                    Grid.Column="2"
                                                    Orientation="Horizontal"
                                                    Spacing="5">
                                                    <TextBlock
                                                        Width="45"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding CurrentDeviceParameter.DO, StringFormat={}{0:F2}}"
                                                        TextTrimming="WordEllipsis"
                                                        ToolTip="{Binding CurrentDeviceParameter.DO, StringFormat={}{0:F2}}" />
                                                    <TextBlock
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="/" />
                                                    <TextBlock
                                                        Width="45"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding CurrentDeviceParameter.DOParam.DO_PV, StringFormat={}{0:F2}}"
                                                        TextTrimming="WordEllipsis"
                                                        ToolTip="{Binding CurrentDeviceParameter.DOParam.DO_PV, StringFormat={}{0:F2}}" />
                                                </hc:UniformSpacingPanel>
                                                <TextBlock
                                                    Grid.Row="4"
                                                    MouseLeftButtonDown="Border_MouseLeftButtonDown"
                                                    Text="转速（RPM）"
                                                    TextAlignment="Left" />
