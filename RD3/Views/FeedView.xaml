                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="10">
                                    <Label
                                        HorizontalAlignment="Left"
                                        HorizontalContentAlignment="Center"
                                        Background="{DynamicResource DefaultBrush}"
                                        BorderBrush="{DynamicResource PrimaryBrush}"
                                        Content="X5"
                                        Foreground="{DynamicResource PrimaryBrush}" />
                                    <ComboBox
                                        Width="120"
                                        BorderBrush="{DynamicResource PrimaryBrush}"
                                        ItemsSource="{Binding Source={StaticResource DataFromEnum1}}"
                                        SelectedItem="{Binding X5}"
                                        Style="{StaticResource ComboBoxExtend}" />
                                </hc:UniformSpacingPanel>
                                <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="10">
                                    <Label
                                        HorizontalAlignment="Left"
                                        HorizontalContentAlignment="Center"
                                        Background="{DynamicResource DefaultBrush}"
                                        BorderBrush="{DynamicResource PrimaryBrush}"
                                        Content="X6"
                                        Foreground="{DynamicResource PrimaryBrush}" />
                                    <ComboBox
                                        Width="120"
                                        BorderBrush="{DynamicResource PrimaryBrush}"
                                        ItemsSource="{Binding Source={StaticResource DataFromEnum1}}"
                                        SelectedItem="{Binding X6}"
                                        Style="{StaticResource ComboBoxExtend}" />
                                </hc:UniformSpacingPanel>
                            </hc:UniformSpacingPanel>
                        </hc:UniformSpacingPanel>
                        <hc:UniformSpacingPanel
                            Grid.Row="4"
                            Orientation="Vertical"
                            Spacing="5">
                            <TextBlock Text="{ex:Language Key='Available function:'}" />
                            <hc:UniformSpacingPanel Orientation="Horizontal" Spacing="150">
                                <hc:UniformSpacingPanel Orientation="Vertical" Spacing="5">
                                    <TextBlock Text="sin(x)" />
                                    <TextBlock Text="cos(x)" />
                                    <TextBlock Text="tan(x)" />
                                    <TextBlock Text="log(a,x)" />
                                    <TextBlock Text="pow(x，y)" />
                                    <TextBlock Text="sqrt(x)" />
                                    <TextBlock Text="cbrt(x)" Visibility="Collapsed" />
                                </hc:UniformSpacingPanel>
                                <hc:UniformSpacingPanel Orientation="Vertical" Spacing="5">
                                    <TextBlock Text="正弦函数" />
                                    <TextBlock Text="余弦函数" />
                                    <TextBlock Text="正切函数" />
                                    <TextBlock Text="以a为底的对数函数" />
                                    <TextBlock Text="x的y次方" />
                                    <TextBlock Text="x的平方根" />
                                    <TextBlock Text="x的立方根" Visibility="Collapsed" />
                                </hc:UniformSpacingPanel>
                            </hc:UniformSpacingPanel>
                        </hc:UniformSpacingPanel>
                    </Grid>
                </TabItem>
            </TabControl>
            <hc:UniformSpacingPanel
                Grid.Row="2"
                Margin="5"
                HorizontalAlignment="Right"
                Orientation="Horizontal"
                Spacing="10">
                <Button
                    Width="80"
                    Height="30"
                    Command="{Binding CloseCommand}"
                    Content="{ex:Language Key='Cancel'}"
                    Style="{StaticResource PrimaryBorderButtonStyle}" />
                <Button
                    Width="80"
                    Height="30"
                    HorizontalAlignment="Right"
                    HorizontalContentAlignment="Center"
                    hc:BorderElement.CornerRadius="{StaticResource DefaultCornerRadius}"
                    Background="{StaticResource PrimaryBrush}"
                    BorderBrush="{StaticResource PrimaryBrush}"
                    Command="{Binding OKCommand}"
                    Content="{ex:Language Key='OK'}"
                    FontSize="{DynamicResource NormalFontSize}"
                    Foreground="White"
                    Visibility="Collapsed" />
            </hc:UniformSpacingPanel>
        </Grid>
    </Border>
</UserControl>
