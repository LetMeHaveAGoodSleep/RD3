                <Button
                    Grid.Row="3"
                    Grid.Column="3"
                    Width="90"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    hc:BorderElement.CornerRadius="15"
                    Background="{StaticResource ButtonBoderBrush}"
                    BorderBrush="{StaticResource ButtonBoderBrush}"
                    Content="开始"
                    Foreground="White" />
                <Button
                    Grid.Row="4"
                    Grid.Column="3"
                    Width="90"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    hc:BorderElement.CornerRadius="15"
                    Background="{StaticResource ButtonBoderBrush}"
