<?xml version="1.0" encoding="utf-8" ?>
<PortManager>
	<pipes>
		<Fpi.Communication.Manager.Pipe>
			<id>cantest</id>
			<name>can测试</name>
			<valid>True</valid>
			<autoConnect>True</autoConnect>
			<autoReConnect>True</autoReConnect>
			<impBus>Fpi.Communication.Buses.TcpClientSocket</impBus>
			<impProtocol>Fpi.Communication.Protocols.Common.Simple.SimpleProtocol</impProtocol>
			<targets>
				<target>cantest</target>
			</targets>
			<propertys>
				<Fpi.Xml.Property>
					<id>Fpi.Communication.Buses.CanBus</id>
					<name>链路配置</name>
					<propertys>
						<Fpi.Xml.Property>
							<id>port</id>
							<name>port</name>
							<value>1</value>
						</Fpi.Xml.Property>

						<Fpi.Xml.Property>
							<id>errorCount</id>
							<name>errorCount</name>
							<value>30</value>
						</Fpi.Xml.Property>

					</propertys>

				</Fpi.Xml.Property>

				<Fpi.Xml.Property>
					<id>Fpi.Communication.Protocols.Common.Simple.SimpleProtocol</id>
					<name>协议配置</name>
				</Fpi.Xml.Property>

				<Fpi.Xml.Property>
					<id>Fpi.Communication.Buses.TcpServer</id>
					<name>链路配置</name>
					<propertys>
						<Fpi.Xml.Property>
							<id>port</id>
							<name>port</name>
							<value>1000</value>
						</Fpi.Xml.Property>

						<Fpi.Xml.Property>
							<id>errorCount</id>
							<name>errorCount</name>
							<value>30</value>
						</Fpi.Xml.Property>

					</propertys>

				</Fpi.Xml.Property>

				<Fpi.Xml.Property>
					<id>Fpi.Communication.Buses.TcpServerForMany</id>
					<name>链路配置</name>
				</Fpi.Xml.Property>

				<Fpi.Xml.Property>
					<id>Fpi.Communication.Buses.TcpClientSocket</id>
					<name>链路配置</name>
					<propertys>
						<Fpi.Xml.Property>
							<id>host</id>
							<name>host</name>
							<value>127.0.0.1</value>
						</Fpi.Xml.Property>

						<Fpi.Xml.Property>
							<id>port</id>
							<name>port</name>
							<value>1000</value>
						</Fpi.Xml.Property>

						<Fpi.Xml.Property>
							<id>errorCount</id>
							<name>errorCount</name>
							<value>30</value>
						</Fpi.Xml.Property>

					</propertys>

				</Fpi.Xml.Property>

			</propertys>

		</Fpi.Communication.Manager.Pipe>

	</pipes>

</PortManager>
